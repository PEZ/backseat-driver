    "flow-typed",
    "_FOSSIL_",
    ".fslckout",
    ".git",
    ".hg",
    ".idea",
    ".joyride",
    "node_modules",
    ".pijul",
    ".project",
    ".shadow-cljs",
    ".stack-work",
    ".svn",
    "target",
    ".tox",
    ".vscode",
